package ex;

import java.util.Scanner;

public class ex_007 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		double A, B, C, pi;
		
		pi = 3.14159;
		
		A = sc.nextDouble();
		B = sc.nextDouble();
		C = sc.nextDouble();
		
		System.out.println("TRIANGULO: " + (A * C) / 2);
		System.out.println("C√çRCULO: " + (A * C) / 2);
		
		sc.close();
		
	}

}
